<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>
            {% block title %}
                {{ (site_meta_title_name is defined ? (site_meta_title_name |trans) ~ " | " : "") ~ 'Test' }}
            {% endblock %}
        </title>

        {% block stylesheets %}
            {{ encore_entry_link_tags('app/css') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app/js') }}
        {% endblock %}

        {% block pageStylesheets %}
        {% endblock %}

        {% block pageJavascripts %}
        {% endblock %}

    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
